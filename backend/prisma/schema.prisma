generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Application {
  id        Int      @id @default(autoincrement())
  company   String
  position  String
  link      String?
  status    String
  notes     String?
  createdAt DateTime @default(now())
}
